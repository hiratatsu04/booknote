root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
indent_size = 4

[*.php]
indent_size = 4

[composer.json]
indent_size = 4

[*.{cs,vb}]
indent_size = 4
end_of_line = crlf

[*.{bat,cmd,ps1}]
end_of_line = crlf

[*.{csproj,vbproj,sln}]
end_of_line = crlf
